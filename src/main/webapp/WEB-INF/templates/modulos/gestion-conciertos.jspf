<%-- any content can be specified here e.g.: --%>
<%@ page pageEncoding="UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<div class="centrar-contenido">
    <table class="table table-striped table-responsive-sm">
        <thead class="thead-dark">
            <tr>
                <th>Nombre</th>
                <th>Artista</th>
                <th>Precio</th>
                <th>DÃ­a</th>
                <th>Hora</th>
                <th></th>
            </tr>
        </thead>
        <tbody>

            <c:forEach var="co" items="${listadoConciertos}">
                <tr>
                    <td>${co.nombre}</td>
                    <td>${co.artista}</td>
                    <td>${co.precio}</td>
                    <td>${co.fecha}</td>
                    <td>${co.hora}</td>
                    <td>
                        <a href="${svlURL}/borraconcierto?concierto-borra=${co.id}" class="btn-primary btn" >Borrar</a>
                        <a href="${svlURL}/editaconcierto?concierto-edita=${co.id}" class="btn-primary btn" >Editar</a>
                    </td>

                </tr>
            </c:forEach>
        </tbody>
    </table>
</div>
